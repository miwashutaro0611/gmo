extends include/_layout
block vars
  include include/_meta
  - var key = "diagnosis"
  - var page = pages[key]
block css
  link(rel="stylesheet", href='./css/ionicons.min.css')
  link(rel="stylesheet", href='./css/style.css')
  link(rel="stylesheet", href='./css/diagnosis.css')
block js
  script(src="./js/plugin/easing.js")
  script(src="./js/main.js")
block loading
  #loader-bg
    #loader
      .loading
        span.loading__title LOADING
        span.loading__circle1
        span.loading__circle2
        span.loading__circle3
block content
  include include/_header
  section.diagnosis_wrap
    #1th.diagnosis_wrap__item
      .question
        .question__query どのような場面で使いますか？
        .question__choice
          label(for="1th_checkbox1").individual_choice
            input#1th_checkbox1.checkbox(type="checkbox" name="1th_check")
            span.lever_word 個人
          label(for="1th_checkbox2").individual_choice
            input#1th_checkbox2.checkbox(type="checkbox" name="1th_check")
            span.lever_word 企業
          label(for="1th_checkbox3").individual_choice
            input#1th_checkbox3.checkbox(type="checkbox" name="1th_check")
            span.lever_word 決めていない
