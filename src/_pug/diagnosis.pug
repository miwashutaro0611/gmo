extends include/_layout
block vars
  include include/_meta
  - var key = "diagnosis"
  - var page = pages[key]
block css
  link(rel="stylesheet", href='./css/ionicons.min.css')
  link(rel="stylesheet", href='./css/style.css')
  link(rel="stylesheet", href='./css/diagnosis.css')
block js
  script(src="./js/main.js")
block content
  include include/_header
  section.diagnosis_wrap
    #1th.diagnosis_wrap__item
      .question
        .question__query
          span.question_number Q1
          span.question_text どのような場面で使いますか？
        .question__choice
          label.individual_choice
            input.checkbox(type="checkbox" name="1th_check")
            span.lever_word 個人
          label.individual_choice
            input.checkbox(type="checkbox" name="1th_check")
            span.lever_word 企業
          label.individual_choice
            input.checkbox(type="checkbox" name="1th_check")
            span.lever_word 決めていない
    #2th.diagnosis_wrap__item
      .question
        .question__query
          span.question_number Q2
          span.question_text どのようなことに利用したいですか？
        .question__choice
          label.individual_choice
            input.checkbox(type="checkbox" name="2th_check")
            span.lever_word 個人
          label.individual_choice
            input.checkbox(type="checkbox" name="2th_check")
            span.lever_word 企業
          label.individual_choice
            input.checkbox(type="checkbox" name="2th_check")
            span.lever_word 決めていない
    #3th.diagnosis_wrap__item
      .question
        .question__query
          span.question_number Q3
          span.question_text 容量はどのくらいにしますか？
